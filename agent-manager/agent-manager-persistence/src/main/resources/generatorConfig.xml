<generatorConfiguration>
    <!-- 数据库驱动 -->
    <classPathEntry location="/Users/didi/.m2/repository/org/mariadb/jdbc/mariadb-java-client/2.5.4/mariadb-java-client-2.5.4.jar"/>
    <context id="BigDataTables" targetRuntime="MyBatis3">
        <!-- 禁用mybatis插件自动生成的注释 -->
        <commentGenerator>
