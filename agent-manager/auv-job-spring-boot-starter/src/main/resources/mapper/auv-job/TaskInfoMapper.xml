<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.didiglobal.logi.auvjob.mapper.AuvTaskMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.didiglobal.logi.auvjob.common.bean.AuvTask">
        <id column="id" property="id" />
        <result column="code" property="code" />
        <result column="name" property="name" />
        <result column="desc" property="desc" />
        <result column="cron" property="cron" />
        <result column="class_name" property="className" />
        <result column="params" property="params" />
        <result column="retry" property="retry" />
        <result column="last_fire_time" property="lastFireTime" />
        <result column="next_fire_time" property="nextFireTime" />
        <result column="timeout" property="timeout" />
        <result column="status" property="status" />
        <result column="sub_task_codes" property="subTaskCodes" />
        <result column="consensual" property="consensual" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, code, name, desc, cron, class_name, params, retry, last_fire_time, next_fire_time, timeout, status, sub_task_codes, consensual
    </sql>

</mapper>
